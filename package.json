    "ts-loader": "^0.8.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrobb/Calcium.git"
  },
  "author": "simonrobb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonrobb/Calcium/issues"
  },
  "homepage": "https://github.com/simonrobb/Calcium",
  "engines": {
    "node": "stable"
  }
}
